HoverClasses].join(\" \");\n\t}\n\n\treturn mainStyling;\n}\n\nfunction assignFontSize(size) {\n\tlet fontSize = \"text-base\";\n\n\tif (size === \"s\") {\n\t\tfontSize = \"text-sm\";\n\t}\n\tif (size === \"m\") {\n\t\tfontSize = \"text-base\";\n\t}\n\tif (size === \"l\") {\n\t\tfontSize = \"text-lg\";\n\t}\n\tif (size === \"xl\") {\n\t\tfontSize = \"text-xl\";\n\t}\n\tif (size === \"2xl\") {\n\t\tfontSize = \"text-2xl\";\n\t}\n\n\treturn fontSize;\n}\n\nexport default function Button({ primary, secondary, hover, size, children }) {\n\tlet buttonStyles = assignMainStyling(primary, secondary, hover);\n\tlet fontSize = assignFontSize(size);\n\n\tbuttonStyles = [buttonStyles, fontSize].join(\" \");\n\n\treturn <button className={buttonStyles}>{children}</button>;\n}\n"},"lineCount":null}},"error":null,"hash":"04dbfb35189e1291f76e8f1e04a467f9","cacheData":{"env":{}}}